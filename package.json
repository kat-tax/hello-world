{
  "name": "HelloWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "web": "vite dev --open",
    "ios": "react-native run-ios --no-packager",
    "macos": "react-native run-macos --no-packager",
    "windows": "react-native run-windows --no-packager",
    "android": "react-native run-android --no-packager",
    "start": "concurrently npm:start:*",
    "start:web": "vite dev",
    "start:native": "react-native start",
    "test": "concurrently npm:test:*",
    "test:unit": "vitest",
    "test:lint": "eslint .",
    "test:type": "tsc --noEmit",
    "build": "concurrently npm:build:*",
    "build:web": "vite build",
    "build:ios": "react-native rnx-bundle --platform ios",
    "build:macos": "react-native rnx-bundle --platform macos",
    "build:windows": "react-native rnx-bundle --platform windows",
    "build:android": "react-native rnx-bundle --platform android",
    "generate": "concurrently npm:generate:*",
    "generate:config": "rnuc config.yaml",
    "generate:patches": "patch-package",
    "generate:translate": "npm run translate:extract && npm run translate:compile",
    "generate:splash": "react-native generate-bootsplash src/assets/images/logo-foreground.svg --background-color=#222",
    "generate:icons": "react-native-svg-app-icon --force",
    "storybook:build": "storybook build",
    "storybook:start": "storybook dev -p 6006",
    "translate:extract": "lingui extract --overwrite --clean",
    "translate:compile": "lingui compile",
    "postinstall": "npm run generate",
    "rnuc": "$(npm bin)/rnuc"
  },
  "dependencies": {
    "@lingui/react": "^4.1.2",
    "@react-native-community/netinfo": "^9.3.10",
    "@react-native-picker/picker": "^2.4.10",
    "@reduxjs/toolkit": "^1.9.5",
    "@syncedstore/core": "^0.5.1",
    "@syncedstore/react": "^0.5.1",
    "history": "^5.3.0",
    "idb-keyval": "^6.2.1",
    "make-plural": "^7.3.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.71.8",
    "react-native-bootsplash": "^4.7.1",
    "react-native-gesture-handler": "^2.10.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-macos": "^0.71.8",
    "react-native-mmkv": "^2.8.0",
    "react-native-reanimated": "^3.1.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-svg": "^13.9.0",
    "react-native-ultimate-config": "^6.0.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-web": "^0.19.4",
    "react-native-windows": "^0.71.13",
    "react-redux": "^8.0.5",
    "react-router": "^6.11.2",
    "redux": "^4.2.1",
    "redux-first-history": "^5.1.1",
    "redux-persist": "^6.0.0",
    "y-indexeddb": "^9.0.11",
    "y-webrtc": "^10.2.5",
    "yjs": "^13.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@lingui/cli": "^4.1.2",
    "@lingui/macro": "^4.1.2",
    "@lingui/vite-plugin": "^4.1.2",
    "@react-native/metro-config": "^0.72.5",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.4",
    "@rnx-kit/cli": "^0.16.9",
    "@rnx-kit/metro-config": "^1.3.6",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.28",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/blocks": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/react-vite": "^7.0.12",
    "@storybook/testing-library": "^0.1.0",
    "@tsconfig/react-native": "^3.0.1",
    "@types/react": "^18.2.6",
    "@types/react-redux": "^7.1.25",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.4",
    "concurrently": "^8.0.1",
    "patch-package": "^7.0.0",
    "prop-types": "^15.8.1",
    "react-native-svg-app-icon": "^0.6.1",
    "react-native-svg-transformer": "^1.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "storybook": "^7.0.12",
    "typescript": "4.8.4",
    "vite": "^4.3.7",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.1"
  },
  "engines": {
    "node": ">=16"
  },
  "browserslist": {
    "production": ">0.2%, not dead, not op_mini all",
    "development": "last 2 versions"
  },
  "rnx-kit": {
    "kitType": "app",
    "bundle": {
      "targets": [
        "android",
        "ios",
        "macos",
        "windows"
      ],
      "platforms": {
        "android": {
          "bundleOutput": "android/main.bundle",
          "sourcemapOutput": "android/main.bundle.map",
          "assetsDest": "android/res"
        },
        "ios": {
          "bundleOutput": "ios/main.jsbundle",
          "sourcemapOutput": "ios/main.jsbundle.map",
          "assetsDest": "ios"
        },
        "macos": {
          "bundleOutput": "macos/main.jsbundle",
          "sourcemapOutput": "macos/main.jsbundle.map",
          "assetsDest": "macos"
        },
        "windows": {
          "bundleOutput": "windows/main.bundle",
          "sourcemapOutput": "windows/main.bundle.map",
          "assetsDest": "windows"
        }
      },
      "plugins": [
        [
          "@rnx-kit/metro-plugin-duplicates-checker",
          {
            "ignoredModules": [
              "react-is"
            ]
          }
        ]
      ]
    }
  }
}

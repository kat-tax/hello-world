    "make-plural": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.68.2",
    "react-native-gesture-handler": "^2.3.4",
    "react-native-mmkv": "^2.4.1",
    "react-native-reanimated": "^2.8.0",

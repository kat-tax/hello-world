{
  "name": "HelloWorld",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "web": "ult-scripts run-web",
    "android": "react-native run-android",
    "windows": "react-native run-windows",
    "macos": "react-native run-macos",
    "ios": "react-native run-ios",
    "build": "ult-scripts build-web",
    "test": "ult-scripts test-web",
    "start": "react-native start --reset-cache",
    "analyze": "source-map-explorer 'build/web/static/js/*.js'",
    "extract": "lingui extract --overwrite --clean",
    "compile": "react-native-svg-app-icon && rnuc .env && lingui compile",
    "generate": "npm run extract && npm run compile",
    "postinstall": "npm run compile"
  },
  "dependencies": {
    "@lingui/react": "^3.14.0",
    "@react-native-community/netinfo": "^9.3.6",
    "@react-native-picker/picker": "^2.4.8",
    "@reduxjs/toolkit": "^1.8.6",
    "history": "^5.3.0",
    "idb-keyval": "^6.2.0",
    "make-plural": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.70.4",
    "react-native-gesture-handler": "2.5.0",
    "react-native-mmkv": "^2.5.1",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-ultimate-config": "^4.0.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-web": "^0.18.10",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.3",
    "redux-first-history": "^5.1.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@lingui/cli": "^3.14.0",
